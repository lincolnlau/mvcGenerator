@(model : <%=name.capitalize%>ViewModel)
@main("Detail"){
<div class="panel panel-info">
	<div class="panel-heading"><h3 class="panel-title"><%= name %></h3></div>
	<div class="panel-body">
		<div class="row">
			<% fields.each do |field| -%>
			<% ps = field.split(":") -%>
			<div class="col-md-2"><%=ps[0]%></div><div class="col-md-10">@model.get<%= ps[0].capitalize %>()</div>
			<%end-%>
		</div>
	</div>
	<div class="panel-footer">
		<a class="btn btn-success active" href="@routes.<%= options[:package] + "." if options[:package]%><%=name.capitalize%>Controller.update(model.getId())">Update</a> <a class="btn btn-link" href="@routes.<%= options[:package] + "." if options[:package]%><%=name.capitalize%>Controller.index(0, 10)">Go Back</a>
	</div>
</div>
}